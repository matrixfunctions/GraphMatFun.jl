using MAT,GraphMatFun,BenchmarkTools;
graph_m7=import_compgraph("simulations/graphs/exp_m7_SID+_3_59.cgr");;
graph_m6=import_compgraph("simulations/graphs/exp_m6_SID+_2_22.cgr");;

graph_m6=Compgraph(Float64,graph_m6);
graph_m7=Compgraph(Float64,graph_m7);


names=["graph_m6","graph_m7"];
graphs=[graph_m6,graph_m7]

for (i,g)=enumerate(graphs)
    n=names[i];
    gen_code("/tmp/$n.c",graph_m6,funname="exp_SID_m6",lang=GraphMatFun.LangC_MKL());


end

gen_code("/tmp/exp_SID_m6.c",graph_m6,funname="exp_SID_m6",lang=GraphMatFun.LangC_MKL());

gen_code("/tmp/exp_SID_m7.c",graph_m7,funname="exp_SID_m7",lang=GraphMatFun.LangC_MKL());
